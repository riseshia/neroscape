<% unless @tsumiges.empty? %>
<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Sellday</th>
      <th>Model</th>
    </tr>
  </thead>

  <tbody>
    <% @tsumiges.each do |tsumige| %>
      <tr>
        <td><%= link_to tsumige.game.name, tsumige.game %></td>
        <td><%= tsumige.game.sellday %></td>
        <td><%= tsumige.game.model %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% end %>