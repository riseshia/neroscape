<p id="notice"><%= notice %></p>

<h1>Listing Creators</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @creators.each do |creator| %>
      <tr>
        <td><%= creator.name %></td>
        <td><%= link_to 'Show', creator %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= will_paginate @creators %>