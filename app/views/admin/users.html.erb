<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th>level</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.name %></td>
        <td><%= user.level ||= link_to "Lv Up", "/admin/lv_up?user_id=#{user.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= will_paginate @users %>