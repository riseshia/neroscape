<h1>Listing creaters</h1>

<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Furigana</th>
    </tr>
  </thead>

  <tbody>
    <% @creaters.each do |creater| %>
      <tr>
        <td><%= link_to creater.name, creater %></td>
        <td><%= creater.furigana %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= will_paginate @creaters %>