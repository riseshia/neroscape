<p id="notice"><%= notice %></p>

<h1>Listing Brands</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Homepage url</th>
      <th>Getchu</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @brands.each do |brand| %>
      <tr>
        <td><%= brand.name %></td>
        <td><%= brand.homepage_url %></td>
        <td><%= brand.getchu_id %></td>
        <td><%= link_to 'Show', brand %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= paginate @brands %>