<p id="notice"><%= notice %></p>

<h1>Listing Characters</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Game</th>
      <th>CV</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @characters.each do |character| %>
      <tr>
        <td><%= character.name %></td>
        <td><%= character.game_title %></td>
        <td><%= character.creator_name %></td>
        <td><%= link_to 'Show', character %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= will_paginate @characters %>