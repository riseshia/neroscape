<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Brand</th>
      <th>Release date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% games.each do |game| %>
      <tr>
        <td><%= link_to game.title, game %></td>
        <td><%= link_to game.brand_name, game.brand %></td>
        <td><%= game.release_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>