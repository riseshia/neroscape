<p id="notice"><%= notice %></p>

<h1>Listing Subgenres</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subgenres.each do |subgenre| %>
      <tr>
        <td><%= subgenre.name %></td>
        <td><%= link_to 'Show', subgenre %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= will_paginate @subgenres %>
